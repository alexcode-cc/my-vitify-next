    "eslint-plugin-vue": "^9.21.1",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "rollup-plugin-regexp": "^5.0.1",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^5.1.3",
    "vite-plugin-vue-meta-layouts": "^0.4.1",
