    "vue-router": "^4.2.5",
    "vuetify": "^3.5.4"
  },
  "devDependencies": {
    "@mdi/js": "^7.4.47",
    "@pinia/testing": "^0.1.3",
    "@playwright/test": "^1.41.2",
    "@rushstack/eslint-patch": "^1.7.2",
    "@testing-library/vue": "^8.0.2",
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.19",
